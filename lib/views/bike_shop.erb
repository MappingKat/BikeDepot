                var rImgHeight = imgWidth * this.height / this.width;
                offsetY = parseInt((rImgHeight - rHeight) / 2);
              } else {
                imgWidth = 'auto';
                imgHeight = $this.parent().height();

                var rImgWidth = imgHeight * this.width / this.height;
                offsetX = parseInt((rImgWidth - rWidth) / 2);
              }

        $this
