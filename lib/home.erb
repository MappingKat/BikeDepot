<script>

// Load video player with Vimeo's Froogaloop.
// Add an event to close when video ends.
var iframe = $('#player')[0],
  player = $f(iframe),
  $videoParts = $('.video-container, #video');

$(function() {

    analytics.trackLink($('#button-start'), 'Clicked Start Link');
    analytics.trackLink($('#button-video'), 'Clicked Video Link');

    player.addEvent('ready', function() {
        player.addEvent('finish', onFinish);
    });

    function onFinish(id) {
        $videoParts.removeClass('visible');
    };

    function togglevideo() {
        if (window.location.hash === '#video') {
            $videoParts.addClass('visible');
            player.api('play');
            window.scrollTo(0, 0);
        } else {
            $videoParts.removeClass('visible');
            player.api('pause');
        }
    };

   $('.close-button, .video-container').click(function(ev) {
        window.location.hash = '#';
        togglevideo();
        return false;
    });

   $('a.video').click(function(ev) {
        window.location.hash = '#video';
        togglevideo();
        return false;
    });

  togglevideo();

});

</script>





